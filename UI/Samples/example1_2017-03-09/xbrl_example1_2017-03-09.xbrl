  <example1:DisclosureOfBankOverdraftAndLoan id="XWAND00000661" contextRef="y2012"> &lt;table cellspacing="0" cellpadding="0"&gt; &lt;tbody&gt; &lt;tr&gt; &lt;td/&gt;
